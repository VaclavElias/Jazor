{
  "compileOnSave": true,
  "compilerOptions": {
    "moduleResolution": "Node",
    "noImplicitAny": false,
    "noEmitOnError": true,
    "removeComments": false,
    "module": "ES2020",
    "sourceMap": false,
    "inlineSourceMap": true,
    "inlineSources": true,
    "target": "es6",
    "outDir": "./../wwwroot/js",
    "typeRoots": [
      "node_modules/@types"
    ]
  },
  "include": [
    "src/**/*",
    "src/**/*.d.ts"
  ],
  "exclude": [
    "node_modules",
    "wwwroot"
  ]
}
